---
import Section from "../Section.astro";

import { work } from "@cv";
---

<Section title="Experience">
  <ul>
    {
      work.map(
        ({ name, startDate, endDate, position, summary, highlights }) => {
          return (
            <li>
              <h3>{name}</h3>
              <h3>{position}</h3>
              <h3>{summary}</h3>
            </li>
          );
        }
      )
    }
  </ul>
</Section>
